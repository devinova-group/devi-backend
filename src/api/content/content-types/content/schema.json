{
  "kind": "collectionType",
  "collectionName": "contents",
  "info": {
    "singularName": "content",
    "pluralName": "contents",
    "displayName": "content",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "img": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "position": {
      "type": "enumeration",
      "enum": [
        "right",
        "left"
      ],
      "default": "right"
    },
    "section": {
      "type": "uid",
      "required": true
    },
    "heading": {
      "type": "component",
      "repeatable": false,
      "component": "component.heading"
    },
    "subheading": {
      "type": "component",
      "repeatable": false,
      "component": "component.heading"
    },
    "description": {
      "type": "component",
      "repeatable": false,
      "component": "component.heading"
    },
    "backgroundimg": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "darkbackgroundimg": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "buttons": {
      "type": "component",
      "repeatable": true,
      "component": "component.button"
    },
    "services": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::service.service"
    },
    "compType": {
      "type": "enumeration",
      "enum": [
        "main",
        "services",
        "contact"
      ],
      "default": "main",
      "required": true
    },
    "gap": {
      "type": "biginteger",
      "default": "19"
    },
    "mobilePosition": {
      "type": "enumeration",
      "enum": [
        "top",
        "bottom"
      ],
      "default": "top"
    },
    "imgObjectFit": {
      "type": "enumeration",
      "enum": [
        "none",
        "contain",
        "cover",
        "fill",
        "scale-down"
      ],
      "default": "none"
    },
    "topsubheading": {
      "type": "component",
      "repeatable": false,
      "component": "component.heading"
    }
  }
}
